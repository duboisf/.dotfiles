{
    "coc.source.around.priority": 10,
    "coc.source.word.priority": 5,
    "coc.source.emoji.priority": 2,
    "coc.source.dictionary.priority": 1,
    "coc.preferences.formatOnType": true,
    "coc.preferences.formatOnTypeFiletypes": [
        "go",
        "json"
    ],
    "coc.preferences.formatOnSaveFiletypes": [
        "go",
        "json"
    ],
    "coc.preferences.extensionUpdateCheck": "weekly",
    "coc.source.emoji.filetypes": [
        "gitcommit",
        "go", // sure, why not?
        "markdown"
    ],
    "diagnostic.checkCurrentLine": true,
    "diagnostic.infoSign": "ღ",
    "diagnostic.hintSign": "ஐ",
    "diagnostic.warningSign": "⚠",
    "diagnostic.errorSign": "✖",
    "diagnostic.enable": true,
    "diagnostic.signOffset": 9999999,
    "diagnostic.highlightOffset": 9999999,
    "diagnostic.refreshOnInsertMode": true,
    "diagnostic.virtualText": false,
    "diagnostic.virtualTextCurrentLineOnly": false,
    "diagnostic.virtualTextPrefix": "  ",
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "go.goplsOptions": {
        "deepCompletion": true,
        "hoverKind": "FullDocumentation",
        "matcher": "Fuzzy",
        "staticcheck": true,
        "symbolMatcher": "Fuzzy",
        "usePlaceholders": true
    }
}
